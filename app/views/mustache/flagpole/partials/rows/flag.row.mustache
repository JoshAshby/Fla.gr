<tr id="{{row.id}}">
  <td>
    {{row.title}}
  </td>

  <td>
    {{row.joineduserID.username}}
  </td>

  <td>
    {{row.formatedDate}}
  </td>

  <td>
    <span class="label">
      {{#row.visibility}}
        <i class="icon-unlock"></i> Public
      {{/row.visibility}}
      {{^row.visibility}}
        <i class="icon-lock"></i> Private
      {{/row.visibility}}
    </span>
  </td>

  <td>
    <span class="label {{#row.reported}}label-error{{/row.reported}}">
      {{#row.reported}}
        <i class="icon-smile"></i> Okay!
      {{/row.reported}}
      {{^row.reported}}
        <i class="icon-ban-circle"></i> Reported!
      {{/row.reported}}
    </span>
  </td>
</tr>
